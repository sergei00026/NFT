{
  "name": "morozova",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start -H 0.0.0.0 -p 3000",
    "format": "prettier --write .",
    "lint": "npx eslint",
    "prepare": "husky && husky install",
    "rename-icons": "node scripts/rename-icons.cjs",
    "generate-icons": "pnpm run rename-icons && npx @svgr/cli --icon --out-dir assets/icons/components --jsx-runtime automatic --typescript --ref --memo --replace-attr-values \"#000=currentcolor\"  --replace-attr-values \"#fff=currentcolor\" -- assets/icons/svg && node scripts/replace-color.cjs",
    "test": "echo \"No tests configured\""
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint . --fix --no-cache --max-warnings=0",
    "*.{js,jsx,ts,tsx,md,mdx}": "prettier --write"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.11.2",
    "clsx": "^2.1.1",
    "framer-motion": "^12.24.0",
    "next": "16.1.1",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-redux": "^9.2.0",
    "swiper": "^12.0.3"
  },
  "devDependencies": {
    "@eslint/js": "9.39.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "7.37.5",
    "globals": "^17.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^16.2.7",
    "prettier": "3.7.1",
    "sass": "^1.97.2",
    "typescript": "^5",
    "typescript-eslint": "8.48.0"
  }
}
